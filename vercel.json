{
  "name": "choseby-healthcare-platform",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "projectSettings": {
          "framework": "nextjs"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://choseby.onrender.com",
    "NEXT_PUBLIC_WS_URL": "wss://choseby.onrender.com/ws"
  }
}